{
    "manifest_version":3,
    "name":"URLmemorize",
    "version":"1.0.0",
    "description":"URLを記憶します",
    "icons":{
        "16":"images/icon_16.png",
        "48":"images/icon_48.png",
        "128":"images/icon_128.png"
    },
    "permissions":[
        "activeTab","scripting","contextMenus","storage","tabs"
    ],
    "action":{
        "default_title":"URLmemorize",
        "default_icon":"images/icon_48.png",
        "default_popup":"files/popup.html"
    },
    "background":{
        "service_worker":"files/background.js"
    }
}